main()

def main() 
    diameter = read("Enter the diameter: ").toNum()
    radius = getRadius(diameter)
    area = getArea(radius)
    circ = getCirc(diameter)
    print("Radius: " + radius + " cm")
    print("Area: " + area + " cm^2")
    print("Circumference: " + circ + " cm")
    if (radius == 5) {
        print("it is average") 
    } else if (radius > 5) {
       print("it is big") 
    } else if (radius < 5) {
        print("it has a good personality ") 
    }

return 

def getRadius(diameter)
    radius = diameter/2
return radius

def getArea(radius)
    area = radius ** 2 * 3.14159265
return area

def getCirc(diameter)
    circ = diameter * 3.14159265
return circ