main()

def main() 
    diameter = read("Enter the diameter: ")
    radius = diameter/2 
    area = findArea(radius)
    circ = findCircumference(diameter)
    print("Radius: " + radius + " cm")
    print("Area: " + area + " cm^2")
    print("Circumference: " + circ + " cm")
    if (radius == 5) {
        print("it is average") 
    } else if (radius > 5) {
       print("it is big") 
    } else if (radius < 5) {
        print("it has a good personality ") 
    }

def findArea(radius) {
    return radius ** 2 * 3.14159265
}
def findCircumference(diameter) {
    return diameter * 3.14159265
}

