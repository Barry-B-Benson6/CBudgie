def main() {
    let test = True
    let diameter = 0
    diameter = cin("Enter the diameter: ")
    let radius = diameter/2 
    let area = findArea(radius)
    let circ = findCircumference(diameter)
    cout("Area: " + area + " cm^2")
    cout("Circumference: " + circ + " cm")
    cout(test)
    test = radius > diameter
    cout(test)
}
def findArea(let radius) {
    return radius ^ 2 * 3.14159265
}
def findCircumference(let diameter) {
    return diameter * 3.14159265
}